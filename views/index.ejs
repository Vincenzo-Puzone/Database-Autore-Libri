<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="/css/style.css" type="text/css">
    <title>Libri</title>
</head>
<body>
    <div class="libri">
        <% libri.forEach(libro => { %>
             <ul>
                <li>Titolo: <%= libro.titolo %> </li>
                <li>Autore: <%= libro.id_autore %> </li>
            </ul>
        <% }) %>
    </div>
    <div class="autori">
        <p>Autori</p>
        <% autori.forEach(autore => { %>
            <ul>
                <li>ID: <%= autore.id %> </li>
                <li>Nome: <%= autore.nome %> </li>
            </ul>
        <% }) %>
    </div>
</body>
</html>